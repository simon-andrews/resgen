{
	"name": "ResGen",
	"version": "1.0",
	"description": "Tailor-made resume generator",
	"browser_action": {
  	"default_popup": "resgen.htm"
  },
	"permissions": ["tabs", "activeTab"],
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"]
		}
	],
	"background": {
		"scripts": ["background.js"]
	},
	"manifest_version": 2
}
